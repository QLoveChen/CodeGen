<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">	<bean id="spy-memcached-conf" class="net.spy.memcached.ConnectionFactoryBuilder">		<!-- 默认是TEXT -->		<property name="protocol" value="BINARY" />		<!-- 默认是ARRAY_MOD -->		<property name="locatorType" value="CONSISTENT" />		<!-- 默认是2500毫秒 -->		<property name="opTimeout" value="${overseas.memcached.opTimeOut}" />	</bean>	<bean id="spy-memcached" class="net.spy.memcached.MemcachedClient"		destroy-method="shutdown">		<constructor-arg>			<bean factory-bean="spy-memcached-conf" factory-method="build"></bean>		</constructor-arg>		<constructor-arg>			<bean class="net.spy.memcached.AddrUtil" factory-method="getAddresses">				<constructor-arg value="${overseas.memcached.url}" />			</bean>		</constructor-arg>	</bean>	<!-- SpyMemcached客户端 -->	<bean id="overseas-sys-CacheClient" class="com.iapppay.utils.cache.SpyMemcachedCacheClient">		<constructor-arg index="0" ref="spy-memcached"/>	</bean>		<bean id="overseas-sys-CacheKey" class="com.iapppay.utils.cache.CacheKey">		<constructor-arg index="0" value="${overseas.cache.base}"/>	</bean></beans>